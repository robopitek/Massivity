<?xml version="1.0" encoding="utf-8"?>
<Patch>

	 <!--= 100% mass is 0.5(mass of Steel) =-->
	
	<!--=============== GU_RedWood "Red wood" is in Alpha Biomes file ====================-->
	
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "AA_SkySteel"]/stuffProps/statFactors</xpath>
			<value>
			<Mass>2</Mass>
			</value>
		</li>
		</operations>
	</Operation>
	
	 <!--= 100% mass is 0.026(mass of Cloth)=-->
	
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "AA_CinderSilk"]/stuffProps/statFactors</xpath>
			<value>
			<Mass>0.96</Mass>
			</value>
		</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "AA_MothSilk"]/stuffProps/statFactors</xpath>
			<value>
			<Mass>0.04</Mass>
			</value>
		</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "AA_GiantCrownedSilkieSilk"]/stuffProps/statFactors</xpath>
			<value>
			<Mass>0.08</Mass>
			</value>
		</li>
		</operations>
	</Operation>

</Patch>