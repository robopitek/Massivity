<?xml version="1.0" encoding="utf-8"?>
<Patch>

	 <!--= 100% mass is 0.5(mass of Steel) =-->
	
	<!--=============== Resources_Wood ====================-->
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "WoodLog_Acacia"]/stuffProps/statFactors</xpath>
			<value>
			<Mass>2.4</Mass>
			</value>
		</li>
		</operations>
	</Operation>
	
	<!--=============== Resources_Lumber ====================-->
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "LumberAcacia"]/stuffProps/statFactors</xpath>
			<value>
			<Mass>1.2</Mass>
			</value>
		</li>
		</operations>
	</Operation>

	
</Patch>