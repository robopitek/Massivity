<?xml version="1.0" encoding="utf-8"?>
<Patch>

	 <!--= 100% mass is 0.5(mass of Steel) =-->
	
	<!--============= GU_RedWood works is for Alpha Animals too ==================-->
	  
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "GU_RedWood"]/stuffProps/statFactors</xpath>
			<value>
			<Mass>0.8</Mass>
			</value>
		</li>
		</operations>
	</Operation>
	
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "AB_MushroomWoodLog"]/stuffProps/statFactors</xpath>
			<value>
			<Mass>0.8</Mass>
			</value>
		</li>
		</operations>
	</Operation>
	
		<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "GU_BlocksRoseQuartz"]/stuffProps/statFactors</xpath>
			<value>
			<Mass>2</Mass>
			</value>
		</li>
		</operations>
	</Operation>
	
		<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "AB_BlocksCragstone"]/stuffProps/statFactors</xpath>
			<value>
			<Mass>2</Mass>
			</value>
		</li>
		</operations>
	</Operation>
	
		<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "AB_BlocksMudstone"]/stuffProps/statFactors</xpath>
			<value>
			<Mass>2</Mass>
			</value>
		</li>
		</operations>
	</Operation>
	
</Patch>